<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/colorSoftFunColorPress"/>
            <corners android:radius="5dp"/>
            <stroke android:width="1px" android:color="@color/colorSoftFunDarkColor"/>
        </shape>
    </item>

    <item>
        <shape>
            <solid android:color="@color/colorSoftFunColor"/>
            <corners android:radius="5dp"/>
            <stroke android:width="1px" android:color="@color/colorSoftFunDarkColor"/>
        </shape>
    </item>
</selector>